@model IEnumerable<Trial.Models.AssignedProjectViewModel>

<div class="row not-assigned-project">
    <h2 class="project-title">ALL ASSIGNED PROJECTS</h2><br /><br />

    <table id="allassigned" class="display">
        <thead>
            <tr>
                <th>PROJECT NAME</th>
                <th>PRICE</th>
                <th>START DATE</th>
                <th>SUBMIT DATE</th>
                <th>DESCRIPTION</th>
                <th>ASSIGNED TO</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {

                <tr>
                    <td>
                        @item.Project.Name
                    </td>
                    <td>
                        @item.Project.Price
                    </td>

                    <td>
                        @item.Project.Shcedule_From
                    </td>
                    <td>
                        @item.Project.Schedule_To
                    </td>

                    <td>
                        @item.Project.Description
                    </td>
                    <td>
                        @item.User.UserName
                    </td>



                </tr>
            }
        </tbody>
    </table>
    <script>
            $(document).ready(function () {
                $("#allassigned").DataTable();
            });

    </script>

</div>

