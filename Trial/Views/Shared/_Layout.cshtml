<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    @RenderSection("head", required: false)

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,intial scale=1" />
    <title>JOBY - Offical Website | Home</title>
    <link rel="stylesheet" type="text/css" href="../CSS/FrameStyle.css">
    <link rel="stylesheet" type="text/css" href="../CSS/MainPage.css" />
    <link rel="stylesheet" type="text/css" href="../CSS/HomeStyle.css" />
    <link rel="stylesheet" type="text/css" href="../CSS/ProfileStyl.css" />
    <link rel="stylesheet" type="text/css" href="../Content/DataTables-1.10.15/css/jquery.dataTables.css">
    <link rel="stylesheet" type="text/css" href="../CSS/toastr.css" />


    <link rel="icon" href="Bikes/Logo3.png">

    <script type="text/javascript" src="../JS/jquery-3.1.0.js"></script>
    <script src="../JS/JColor.js"></script>
    <script src="../JS/jMouse.js"></script>
    <script type="text/javascript" src="../JS/Gotop.js"></script>
    <script type="text/javascript" src="../Content/DataTables-1.10.15/js/jquery.dataTables.js"></script>
    <script src="../JS/toastr.js"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

    <!-- ************************* Bootstrap Links ************************* -->

    <link rel="stylesheet" type="text/css" href="../JS/bootstrap-3.3.7-dist/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="../JS/bootstrap-3.3.7-dist/css/bootstrap-theme.css">
    <link rel="stylesheet" type="text/css" href="../JS/bootstrap-3.3.7-dist/css/bootstrap-theme.min.css">
    <link rel="stylesheet" type="text/css" href="../JS/bootstrap-3.3.7-dist/css/bootstrap.min.css">

    <script type="text/javascript" src="../JS/bootstrap-3.3.7-dist/js/npm.js"></script>
    <script type="text/javascript" src="../JS/bootstrap-3.3.7-dist/js/bootstrap.min.js"></script>
</head>
<body>

    <div class="container-fluid">
        <nav class="n-color navbar navbar-toggleable-md navbar-fixed-top navbar-light bg-faded">
            <button class="navbar-toggle navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="glyphicon glyphicon-align-justify" aria-hidden="true"></span>
            </button>
            <a style="background-color: #283345; width: 100%;" class="navbar-brand" href="~/Home/Main"><b class="slogan-position">JOBY</b></a>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item active title-2"><a class="nav-link title" href="~/Home/Main">HOME</a></li>
                    <li class="nav-item title-2"><a class="nav-link title" href="~/Home/OurTeam">OUR TEAM</a></li>
                    <li class="nav-item title-2"><a class="nav-link title" href="#">HIRE</a></li>
                    <li class="nav-item title-2"><a class="nav-link title" href="#">POST JOB</a></li>

                </ul>
                <ul class="nav navbar-nav navbar-right">
                    @RenderSection("Loginbtn", required: false)
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </nav>

        @RenderSection("Popups", required: false)
        <!-- ***************************** End Header ******************************** -->
        
        @RenderSection("JobForm", required: false)

        @RenderBody()
        @RenderSection("Loggedin", required: false)
        @RenderSection("Guest", required: false)
        @RenderSection("notassigned", required: false)
        @RenderSection("footer", required: false)
        @Scripts.Render("~/bundles/bootstrap")


    </div>
</body>
</html>
